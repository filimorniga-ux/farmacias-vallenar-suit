{
  "name": "farmacias-vallenar-suit",
  "private": true,
  "version": "1.0.4",
  "description": "Sistema de gesti√≥n integral para Farmacias Vallenar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filimorniga-ux/farmacias-vallenar-suit.git"
  },
  "author": "Farmacias Vallenar",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build:desktop": "electron-builder",
    "clean:desktop": "rm -rf dist-desktop",
    "checkpoint": "git add . && git commit -m \"Auto Checkpoint $(date)\" && git push",
    "seed:demo": "tsx src/scripts/seed-demo-vallenar.ts",
    "terminals:status": "tsx src/cli/terminals.ts status",
    "terminals:health": "tsx src/cli/terminals.ts health",
    "terminals:force-close": "tsx src/cli/terminals.ts force-close",
    "terminals:cleanup": "tsx src/cli/terminals.ts cleanup",
    "migrate:pins": "tsx src/scripts/migrate-pins-to-bcrypt.ts",
    "security:audit": "tsx src/scripts/audit-system-health.ts"
  },
  "build": {
    "appId": "com.farmaciasvallenar.erp",
    "productName": "Farmacias Vallenar Suit",
    "publish": [
      "github"
    ],
    "mac": {
      "category": "public.app-category.business",
      "target": [
        {
          "target": "dmg",
          "arch": "universal"
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "public/icon.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.png"
    },
    "directories": {
      "output": "dist-desktop"
    },
    "files": [
      "electron/**/*",
      "public/**/*",
      "package.json"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.89",
    "@headlessui/react": "^2.2.9",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/react-router-dom": "^5.3.3",
    "ai": "^6.0.34",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "electron-updater": "^6.7.3",
    "eslint-config-next": "^16.0.7",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^5.3.3",
    "framer-motion": "^12.23.25",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.554.0",
    "next": "^16.0.7",
    "next-pwa": "^5.6.0",
    "nodemailer": "^7.0.11",
    "openai": "^6.16.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.9.6",
    "react-to-print": "^3.2.0",
    "recharts": "^3.5.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "workbox-window": "^7.4.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.10.6",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-dropzone": "^4.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  }
}