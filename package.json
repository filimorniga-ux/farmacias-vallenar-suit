{
  "name": "farmacias-vallenar-suit",
  "private": true,
  "version": "1.0.10",
  "description": "Sistema de gesti√≥n integral para Farmacias Vallenar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filimorniga-ux/farmacias-vallenar-suit.git"
  },
  "author": "Farmacias Vallenar",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "seed:sandbox": "npx tsx src/scripts/seed-sandbox.ts",
    "test:actions": "vitest run tests/actions",
    "test:actions:coverage": "vitest run tests/actions --coverage --coverage.include=\"src/actions/**\"",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "build:desktop": "electron-builder",
    "build:mac:arm64": "electron-builder --mac --arm64",
    "build:win": "electron-builder --win --x64",
    "build:mobile": "npm run build && npx cap sync",
    "clean:desktop": "rm -rf dist-desktop",
    "checkpoint": "node scripts/checkpoint.mjs",
    "seed:demo": "tsx src/scripts/seed-demo-vallenar.ts",
    "db:seed:local": "tsx src/scripts/seed-demo-vallenar.ts",
    "test:integration": "vitest run tests/integration",
    "terminals:status": "tsx src/cli/terminals.ts status",
    "terminals:health": "tsx src/cli/terminals.ts health",
    "terminals:force-close": "tsx src/cli/terminals.ts force-close",
    "terminals:cleanup": "tsx src/cli/terminals.ts cleanup",
    "migrate:pins": "tsx src/scripts/migrate-pins-to-bcrypt.ts",
    "security:audit": "tsx src/scripts/audit-system-health.ts"
  },
  "build": {
    "appId": "com.farmaciasvallenar.erp",
    "productName": "Farmacias Vallenar Suit",
    "npmRebuild": false,
    "publish": [
      "github"
    ],
    "mac": {
      "category": "public.app-category.business",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "identity": "Miguel Alfonso Perdomo serrato (XY5KRY6FXK)",
      "artifactName": "FarmaciasVallenar-Mac-${arch}.${ext}"
    },
    "afterSign": "build/notarize.cjs",
    "dmg": {
      "sign": false
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.ico",
      "executableName": "FarmaciasVallenarSuit",
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "artifactName": "FarmaciasVallenar-Setup.exe"
    },
    "directories": {
      "output": "dist-desktop"
    },
    "files": [
      "electron/**/*",
      "public/**/*",
      "package.json",
      "!data/**/*",
      "!data_imports/**/*",
      "!dist-desktop/**/*"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.89",
    "@capacitor/android": "^8.0.2",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.9",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.34.0",
    "@tanstack/query-sync-storage-persister": "^5.90.20",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-query-persist-client": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/react-router-dom": "^5.3.3",
    "ai": "^6.0.34",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.2",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "eslint-config-next": "^16.0.7",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^5.3.3",
    "framer-motion": "^12.23.25",
    "html5-qrcode": "^2.3.8",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.554.0",
    "next": "^16.0.7",
    "next-pwa": "^5.6.0",
    "nodemailer": "^7.0.11",
    "openai": "^6.16.0",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.9.6",
    "react-to-print": "^3.2.0",
    "recharts": "^3.5.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "workbox-window": "^7.4.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@_davideast/stitch-mcp": "^0.3.2",
    "@capacitor/assets": "^3.0.5",
    "@electron/notarize": "^3.1.1",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.10.6",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-dropzone": "^4.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "png2icons": "^2.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  }
}
